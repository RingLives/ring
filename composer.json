{
    "name": "ringlives/ring",
    "description": "Ringlives is a content management system.",
    "keywords": ["laravel", "ringlives", "ring", "laravel-cms"],
    "autoload": {
        "files": [
            "src/Supports/helpers.php"
        ],
        "psr-4": {
            "Ringlives\\Ring\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ringlives",
            "email": "shohidurr49@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Ringlives\\Ring\\RingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
